# http://editorconfig.org

root = true

[*] #目录下的所有文件
charset = utf-8 #使用的字符集
indent_style = space #缩进
indent_size = 2 #缩进的单位为两个空格的大小
end_of_line = lf #一行的结束使用linux风格
insert_final_newline = true #最后一行写完后换行
trim_trailing_whitespace = true #删除一行结束的空格

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false
